{
	"name": "gamerchat",
	"version": "1.0.0",
	"description": "CS 314 Project",
	"scripts": {
		"start": "node server/server.js",
		"server": "nodemon /server/server.js",
		"client": "npm run dev --prefix client && start http://localhost:3000",
		"dev": "concurrently \"npm run server\" \"npm run client\" ",
		"start-chat-server": "node server/index.js",
		"test": "jest",
		"test:client": "jest --selectProjects client",
		"test:server": "jest --selectProjects server",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"heroku-postbuild": "cd client && npm install && npm run build",
		"clean-install": "rm -rf node_modules && rm -rf client/node_modules && rm package-lock.json && rm client/package-lock.json && npm install && cd client && npm install"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/preset-env": "^7.24.6",
		"@babel/preset-react": "^7.24.6",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^15.0.7",
		"@testing-library/user-event": "^14.5.2",
		"babel-jest": "^29.7.0",
		"concurrently": "^8.2.2",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-fetch-mock": "^3.0.3",
		"nodemon": "^3.1.0",
		"setimmediate": "^1.0.5",
		"supertest": "^7.0.0",
		"wait-on": "^7.2.0",
		"whatwg-fetch": "^3.6.20"
	},
	"dependencies": {
		"dotenv": "^16.4.5",
		"bcryptjs": "^2.4.3",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"express": "^4.19.2",
		"express-async-handler": "^1.2.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.3.3",
		"multer": "^1.4.5-lts.1",
		"socket.io": "^4.7.5"
	}
}
